sequence notice_id_sequence increment by 1;

table notices(
 id int,                     
 date text,                 //last modified date
 expire_date text,
 subject text,
 reference text,
 sent_from text,
 sent_to text,
 user_id text,
 emailsend text default 'no',
 re_edited text default 'no',
 date_created text          //date when notice created first time
);

table old_notices : notices;

table trash_notices : notices(
 trash_date timestamp default now()
); 

table student_preference(
 enrol_no int,
 email_id text,
 sent_to text,
 key text
)

table log_session(
 sid text not null,
 userid text,
 name text
)
